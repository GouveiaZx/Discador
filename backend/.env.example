# Configurações do Banco de Dados
DATABASE_URL=postgresql://usuario:senha@host:porta/database

# Configurações do Supabase
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_ANON_KEY=sua_chave_anonima_aqui
SUPABASE_SERVICE_KEY=sua_chave_de_servico_aqui

# Configurações do Servidor
PORT=8000
HOST=0.0.0.0
DEBUG=true

# Configurações JWT
JWT_SECRET_KEY=sua_chave_secreta_jwt_aqui
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# Configurações de CORS
# Adicione aqui os domínios que devem ter acesso ao backend
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173","https://discador.vercel.app","https://seu-frontend.vercel.app"]

# Configurações do Asterisk
ASTERISK_HOST=localhost
ASTERISK_PORT=5038
ASTERISK_USERNAME=admin
ASTERISK_PASSWORD=admin

# Configurações de Log
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE=logs/discador.log
LOG_ROTATION=true
LOG_MAX_SIZE_MB=10
LOG_BACKUP_COUNT=5